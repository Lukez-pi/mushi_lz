Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"p", "[", 
    RowBox[{"n_", ",", "k_", ",", "b_"}], "]"}], "=", 
   RowBox[{
    FractionBox[
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"n", "-", "b", "-", "1"}], ")"}], "!"}], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"b", "-", "1"}], ")"}], "!"}]}], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"n", "-", "1"}], ")"}], "!"}]], 
    RowBox[{"(", 
     RowBox[{"k", "-", "1"}], ")"}], " ", 
    RowBox[{"Binomial", "[", 
     RowBox[{
